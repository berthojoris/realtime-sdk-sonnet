{
  "name": "realtime-analytics-sdk",
  "version": "1.0.1",
  "description": "Real-time analytics SDK for Node.js with multi-database support and automatic SQLite directory creation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/berthojoris/realtime-sdk-sonnet.git"
  },
  "bugs": {
    "url": "https://github.com/berthojoris/realtime-sdk-sonnet/issues"
  },
  "homepage": "https://github.com/berthojoris/realtime-sdk-sonnet#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "SETUP_GUIDE.md",
    ".env.example"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/server/server.js",
    "start:dev": "npm run build && node dist/server/server.js",
    "migrate": "npm run build && node dist/migrations/cli.js",
    "migrate:up": "npm run migrate up",
    "migrate:down": "npm run migrate down",
    "migrate:status": "npm run migrate status",
    "migrate:fresh": "npm run migrate fresh",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "analytics",
    "tracking",
    "real-time",
    "events",
    "mongodb",
    "mysql",
    "postgresql",
    "sqlite",
    "metrics",
    "database",
    "multi-database",
    "environment-config",
    "connection-string",
    "migrations",
    "gdpr",
    "analytics-server",
    "event-tracking"
  ],
  "author": "Bertho Joris <berthojoris@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.6.1",
    "eventemitter3": "^5.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "mongodb": "^6.3.0",
    "mysql2": "^3.6.5",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "better-sqlite3": "^9.2.2",
    "mongodb": "^6.3.0",
    "mysql2": "^3.6.5",
    "pg": "^8.11.3"
  },
  "peerDependenciesMeta": {
    "mysql2": {
      "optional": true
    },
    "pg": {
      "optional": true
    },
    "mongodb": {
      "optional": true
    },
    "better-sqlite3": {
      "optional": true
    }
  }
}
